object dmQuestions: TdmQuestions
  OldCreateOrder = False
  OnCreate = DataModuleCreate
  Height = 326
  Width = 377
  object qryQuestionsGetItem: TFDQuery
    SQL.Strings = (
      'select * from questions where que_id = :id')
    Left = 144
    Top = 48
    ParamData = <
      item
        Name = 'ID'
        ParamType = ptInput
      end>
  end
  object qryQuestionsPostItem: TFDQuery
    Left = 144
    Top = 120
  end
end
